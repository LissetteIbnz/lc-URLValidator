{
  "name": "library-ts-boilerplate",
  "version": "1.0.0",
  "description": "NPM Library boilerplate with TS",
  "keywords": [
    "lc-form-validation",
    "form",
    "validation",
    "validator",
    "react",
    "component"
  ],
  "homepage": "https://github.com/Lemoncode/lc-validator-dni#readme",
  "bugs": {
    "url": "https://github.com/Lemoncode/lc-validator-dni/issues"
  },
  "license": "MIT",
  "author": "Lemoncode",
  "contributors": [
    "Javier Calzado <javi.calzado@lemoncode.net> (https://github.com/fjcalzado)"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "index.d.ts",
    "LICENSE",
    "package.json",
    "readme.md"
  ],
  "browser": "lib/index.js",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "index.d.ts",
  "jsnext:main": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lemoncode/lc-validator-dni.git"
  },
  "scripts": {
    "clean": "rimraf lib dist es",
    "build": "npm run clean && npm run build:lib && npm run build:lib:esm && npm run build:umd && npm run build:umd:min && npm run build:es",
    "build:lib": "cross-env BABEL_ENV=es5_cjs babel ./src --out-dir ./lib --copy-files --extensions '.ts,.js' --ignore spec.js,spec.ts,test.js,test.ts",
    "build:lib:esm": "cross-env BABEL_ENV=es5_esm babel ./src/index.ts --out-file ./lib/index.esm.js",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./es --copy-files --extensions '.ts,.js' --ignore spec.js,spec.ts,test.js,test.ts",
    "build:umd": "cross-env BABEL_ENV=umd NODE_ENV=development webpack",
    "build:umd:min": "cross-env BABEL_ENV=umd NODE_ENV=production webpack -p",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "prerelease": "npm run clean && npm run build",
    "release": "npm run prerelease && npm run publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "setupFiles": [],
    "restoreMocks": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/es/",
      "/lib/"
    ]
  }
}
